{
  "expo": {
    "name": "MitraAkucuciin",
    "slug": "akucuciin-mitra-apps",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LogoAkucuciin2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LogoAkucuciin2.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.akucuciin.mitra"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/LogoAkucuciin2.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/LogoAkucuciin2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "android": {
            "fonts": ["./assets/fonts/Montserrat-Regular.ttf", "./assets/fonts/Montserrat-Bold.ttf"]
          },
          "ios": {
            "fonts": ["./assets/fonts/Montserrat-Regular.ttf", "./assets/fonts/Montserrat-Bold.ttf"]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3ce10af8-be52-4fb4-b248-c042667e0c46"
      }
    },
    "owner": "27mahes",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/3ce10af8-be52-4fb4-b248-c042667e0c46"
    }
  }
}

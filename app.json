{
  "expo": {
    "name": "Mitra Laundry Akucuciin",
    "slug": "Mitra Laundry Akucuciin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LogoAkucuciin2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LogoAkucuciin2.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/LogoAkucuciin2.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/LogoAkucuciin2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            {
              "fontFamily": "Montserrat",
              "fontFile": "./assets/fonts/Montserrat-Regular.ttf"
            }
          ],
          "android": {
            "fonts": [
              {
                "fontFamily": "Montserrat",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Montserrat-Regular.ttf",
                    "weight": 500
                  },
                  {
                    "path": "./assets/fonts/Montserrat-Bold.ttf",
                    "weight": 700
                  }
                ]
              }
            ]
          },
          "ios": {
            "fonts": [
              "./assets/fonts/Montserrat-Regular.ttf",
              "./assets/fonts/Montserrat-Bold.ttf"
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
